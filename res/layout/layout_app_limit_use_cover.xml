<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/sup_con_limit_use_cover_content"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <View
        android:id="@+id/sup_con_limit_use_cover"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#85000000" />

    <ImageView
        android:id="@+id/sup_con_limit_use_image"
        android:layout_width="325dp"
        android:layout_height="325dp"
        android:layout_gravity="center"
        android:layout_marginBottom="110dp"
        android:src="@mipmap/hc_mobile_limit_use_info" />


    <View
        android:id="@+id/sup_con_limit_use_url"
        android:layout_width="300dp"
        android:layout_height="40dp"
        android:layout_gravity="center"
        android:layout_marginBottom="8dp" />

    <ImageView
        android:id="@+id/sup_con_limit_use_close"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_gravity="center"
        android:layout_marginTop="95dp"
        android:src="@mipmap/hc_mobile_limit_use_close" />


</FrameLayout>